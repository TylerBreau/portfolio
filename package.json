{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "main": "./www/index.html",
  "scripts": {
    "lint": "eslint ./src --ext .js,.jsx,.ts",
    "webpack": "webpack -c ./webpack.config.js",
    "copy-statics": "cp ./src/index.html ./www/index.html && cp ./src/res/favicon.ico ./www/favicon.ico",
    "build": "npm run lint && npm run copy-statics && npm run webpack"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.17.0",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@types/prop-types": "^15.7.4",
    "@types/react": "17.0.39",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "babel-loader": "8.2.3",
    "css-loader": "6.6.0",
    "eslint": "7.32.0",
    "eslint-config-standard": "16.0.3",
    "eslint-import-resolver-webpack": "0.13.2",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.2.0",
    "eslint-plugin-react": "7.28.0",
    "less": "4.1.2",
    "less-loader": "10.2.0",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "6.2.1",
    "style-loader": "3.3.1",
    "ts-loader": "9.2.6",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "typescript": "4.5.5",
    "webpack": "5.68.0",
    "webpack-cli": "4.9.2"
  }
}
